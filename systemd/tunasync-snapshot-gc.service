[Unit]
Description=Delete garbage subvolumes generated by tunasync
Requires = network.target
After = network.target

[Service]
Type=oneshot
ExecStart=/home/mirror/.virtualenvs/tunasync/bin/python -u /home/mirror/tunasync/tunasync_snapshot_gc.py -c /etc/tunasync.conf

[Install]
WantedBy = multi-user.target
